{
  "name": "arteflix",
  "namespace": false,
  "jsx": {
    "version": 4
  },
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "rescript-schema",
    "rescript-nodejs",
    "@glennsl/rescript-fetch",
    "rescript-swr"
  ],
  "bsc-flags": ["-open RescriptCore", "-open RescriptSchema"],
  "ppx-flags": ["rescript-schema-ppx/bin"],
  "sources": [
    {
      "dir": "/src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".gen.mjs",
  "gentypeconfig": {
    "language": "untyped",
    "shims": []
  }
}
