{
  "name": "arteflix",
  "namespace": false,
  "jsx": {
    "version": 4
  },
  "bs-dependencies": [
    "@rescript/core",
    "sury",
    "rescript-nodejs",
    "@glennsl/rescript-fetch",
    "rescript-webapi"
  ],

  "bsc-flags": ["-open RescriptCore"],
  "ppx-flags": ["sury-ppx/bin"],
  "sources": [
    {
      "dir": "src"
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".bs.mjs",
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.tsx",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
